#include <iostream>
using namespace std;
int main()
{
    cout <<"size of char:"<< size of (char)<<"byte"<< end;
    cout <<"size of int:"<< size of (int)<<"bytes"<< end;
    cout <<"size of float:"<< size of (float)<<"bytes"<< end;
    cout <<"size of double:"<< size of (double)<<"bytes"<< end;
    system("pause");
}
